using System;

namespace SISAR.Entidades
{
	public class T_Zona
	{
		#region Fields

		private int id_Zona;
		private string nombre;
		private string descripcion;
		private int id_Local;
		private int aforo;

		#endregion

		#region Constructors

		/// <summary>
		/// Initializes a new instance of the T_Zona class.
		/// </summary>
		public T_Zona()
		{
		}

		/// <summary>
		/// Initializes a new instance of the T_Zona class.
		/// </summary>
		public T_Zona(string nombre, string descripcion, int id_Local, int aforo)
		{
			this.nombre = nombre;
			this.descripcion = descripcion;
			this.id_Local = id_Local;
			this.aforo = aforo;
		}

		/// <summary>
		/// Initializes a new instance of the T_Zona class.
		/// </summary>
		public T_Zona(int id_Zona, string nombre, string descripcion, int id_Local, int aforo)
		{
			this.id_Zona = id_Zona;
			this.nombre = nombre;
			this.descripcion = descripcion;
			this.id_Local = id_Local;
			this.aforo = aforo;
		}

		#endregion

		#region Properties
		/// <summary>
		/// Gets or sets the Id_Zona value.
		/// </summary>
		public virtual int Id_Zona
		{
			get { return id_Zona; }
			set { id_Zona = value; }
		}

		/// <summary>
		/// Gets or sets the Nombre value.
		/// </summary>
		public virtual string Nombre
		{
			get { return nombre; }
			set { nombre = value; }
		}

		/// <summary>
		/// Gets or sets the Descripcion value.
		/// </summary>
		public virtual string Descripcion
		{
			get { return descripcion; }
			set { descripcion = value; }
		}

		/// <summary>
		/// Gets or sets the Id_Local value.
		/// </summary>
		public virtual int Id_Local
		{
			get { return id_Local; }
			set { id_Local = value; }
		}

		/// <summary>
		/// Gets or sets the Aforo value.
		/// </summary>
		public virtual int Aforo
		{
			get { return aforo; }
			set { aforo = value; }
		}

		#endregion
	}
}
