        <inputToolkit:TimePicker TimeUpDownStyle="{StaticResource TimeUpDownStyle1}" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="3" Grid.Row="2" Width="120"/>
        <inputToolkit:NumericUpDown HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="3" Grid.Row="4" Width="120" MinHeight="1" Maximum="24"/>
        
    </Grid>
