using System;

namespace SISAR.Entidades
{
	public class T_Menu
	{
		#region Fields

		private int id_Menu;
		private string nombre;
		private string descripcion;
		private decimal precioUnitario;
		private string observacion;

		#endregion

		#region Constructors

		/// <summary>
		/// Initializes a new instance of the T_Menu class.
		/// </summary>
		public T_Menu()
		{
		}

		/// <summary>
		/// Initializes a new instance of the T_Menu class.
		/// </summary>
		public T_Menu(string nombre, string descripcion, decimal precioUnitario, string observacion)
		{
			this.nombre = nombre;
			this.descripcion = descripcion;
			this.precioUnitario = precioUnitario;
			this.observacion = observacion;
		}

		/// <summary>
		/// Initializes a new instance of the T_Menu class.
		/// </summary>
		public T_Menu(int id_Menu, string nombre, string descripcion, decimal precioUnitario, string observacion)
		{
			this.id_Menu = id_Menu;
			this.nombre = nombre;
			this.descripcion = descripcion;
			this.precioUnitario = precioUnitario;
			this.observacion = observacion;
		}

		#endregion

		#region Properties
		/// <summary>
		/// Gets or sets the Id_Menu value.
		/// </summary>
		public virtual int Id_Menu
		{
			get { return id_Menu; }
			set { id_Menu = value; }
		}

		/// <summary>
		/// Gets or sets the Nombre value.
		/// </summary>
		public virtual string Nombre
		{
			get { return nombre; }
			set { nombre = value; }
		}

		/// <summary>
		/// Gets or sets the Descripcion value.
		/// </summary>
		public virtual string Descripcion
		{
			get { return descripcion; }
			set { descripcion = value; }
		}

		/// <summary>
		/// Gets or sets the PrecioUnitario value.
		/// </summary>
		public virtual decimal PrecioUnitario
		{
			get { return precioUnitario; }
			set { precioUnitario = value; }
		}

		/// <summary>
		/// Gets or sets the Observacion value.
		/// </summary>
		public virtual string Observacion
		{
			get { return observacion; }
			set { observacion = value; }
		}

		#endregion
	}
}
