<basics:ChildWindow
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:basics="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
	xmlns:expressionLight="clr-namespace:System.Windows.Controls.Theming;assembly=System.Windows.Controls.Theming.ExpressionLight"
	x:Class="SISAR.WebAnimation.Ubicanos"
	Title="Ubicanos"
	Width="600" Height="400">
	<expressionLight:ExpressionLightTheme VerticalAlignment="Top">
		<Grid x:Name="LayoutRoot">
			<Image Height="280" Margin="0,0,18,-296" VerticalAlignment="Bottom" Source="Imagenes/Ubicanos.png" Stretch="Fill" HorizontalAlignment="Right" Width="234">
				<Image.Effect>
					<DropShadowEffect Color="#FF999999" Opacity="0.57" ShadowDepth="23" Direction="327" BlurRadius="68"/>
				</Image.Effect>
				<Image.Projection>
					<PlaneProjection RotationY="27"/>
				</Image.Projection>
			</Image>	
		
		</Grid>	
	</expressionLight:ExpressionLightTheme>
		
	
</basics:ChildWindow>